{
    "name" : "scoutapp/scoutapm-php",
    "description": "A PHP agent for Scout",
    "require" : {
        "php" : ">= 7.1",
        "ramsey/uuid": "^3.7",
        "ralouphie/getallheaders": "2.0.5",
        "ext-json": "*",
        "ext-sockets": "*",
        "psr/log": "^1.1"
    },
    "require-dev" : {
        "phpunit/phpunit" : "7.*",
        "spatie/phpunit-watcher": "^1.8",
        "friendsofphp/php-cs-fixer": "^2.15"
    },
    "suggest": {
        "ext-xdebug": "Required for processing of request headers"
    },
    "autoload" : {
        "psr-4" : {
            "Scoutapm\\": "src/"
        }
    },
    "autoload-dev": {
    "psr-4": {
        "Scoutapm\\Tests\\": "tests/"
    }
  },
    "config" : {
        "optimize-autoloader" : true
    },
    "provide": {
        "psr/log-implementation": "1.0.0"
    }
}
