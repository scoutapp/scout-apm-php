{
    "name" : "scoutapp/scoutapm-php",
    "description": "A PHP agent for Scout",
    "require" : {
        "php" : ">= 7.1",
        "ramsey/uuid": "^3.7",
        "ralouphie/getallheaders": "2.0.5",
        "ext-json": "*",
        "ext-sockets": "*"
    },
    "require-dev" : {
        "phpunit/phpunit" : "6.*"
    },
    "suggest": {
        "ext-xdebug": "Required for processing of request headers"
    },
    "autoload" : {
        "psr-4" : {
            "Scoutapm\\": "src/"
        }
    },
    "autoload-dev": {
    "psr-4": {
        "Scoutapm\\Tests\\": "tests/"
    }
  },
    "config" : {
        "optimize-autoloader" : true
    }
}
